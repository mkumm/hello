{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.1",
    "manifest_version": 4, 
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    }, 
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    }
}
